{
  "version": 2,
  "name": "qr-editor",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_APP_NAME": "QR Editor",
    "NEXT_PUBLIC_APP_VERSION": "1.0.0"
  },
  "regions": ["iad1"],
  "cleanUrls": true,
  "trailingSlash": false
